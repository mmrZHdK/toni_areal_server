{{#if this.length}}
    <ul class="list list-room">
        {{#each this}}
            <li class="list-element">
                <a href="#" class="js-selectRoom clearfix" data-id="{{id}}">
                    <span class="room-number">{{roomNrHumanShort}}</span>
                    <span class="room-label">{{roomLabelHuman}}</span>
                    <span class="room-navigate icon-arrow-right"></span>
                </a>
            </li>
        {{/each}}
    </ul>
{{else}}
    <!--<p>Keine RÃ¤ume gefunden...</p>-->
{{/if}}
